<view><view class="top"><view class="top-title" style="{{(topStyle)}}">交工督办系统</view><view class="top-center"><view class="top-center-content"><view></view></view><view class="top-center-bottom"><block wx:if="{{loginStatus}}"><view class="top-center-bottom-name">{{userInfo.adminName}}</view></block><block wx:if="{{!loginStatus}}"><view class="top-center-bottom-name">未登录</view></block><view class="top-center-bottom-msg">欢迎使用交工督办系统</view></view></view></view><view class="center"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view class="center-content"><view class="center-content-left"><image src="{{item.imageUrl}}"></image></view><view data-event-opts="{{[['tap',[['clickCenterBut',['$0'],[[['list','id',item.id]]]]]]]}}" class="center-content-right" bindtap="__e"><view class="center-content-right-l">{{item.titleName}}</view><view class="center-content-right-r"><image src="../../static/rightr.png"></image></view></view></view></block></view><view class="bottom"><block wx:if="{{loginStatus}}"><view data-event-opts="{{[['tap',[['loginOutBut',['$event']]]]]}}" class="bottom-top" bindtap="__e">退出登录</view></block><block wx:if="{{!loginStatus}}"><view data-event-opts="{{[['tap',[['clickLoginBut',['$event']]]]]}}" class="bottom-top" bindtap="__e">登录</view></block></view></view>